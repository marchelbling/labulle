oraine.", "cover": "https://bdi.dlpdomain.com/album/9782800138541-couv-M800x1600.jpg", "samples": ["https://bdi.dlpdomain.com/album/9782800138541-page5-M800x1600.jpg", "https://bdi.dlpdomain.com/album/9782800138541-page6-M800x1600.jpg", "https://bdi.dlpdomain.com/album/9782800138541-page7-M800x1600.jpg", "https://bdi.dlpdomain.com/album/9782800138541-page8-M800x1600.jpg", "https://bdi.dlpdomain.com/album/9782800138541-page9-M800x1600.jpg"], "pages": 80, "date": "2008-01-09", "price": {"amount": 16.5, "currency": "eur"}, "isbn": "9782800138541", "illustrators": ["Berberian", "Dupuy"], "writers": ["Denis"], "website": null, "black_and_white": false, "age": 12, "genre": ["roman graphique", "humour"], "book": "album cartonné", "width": 237, "height": 310}
